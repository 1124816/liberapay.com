# Inject version number into archives
liberapay/version.py  export-subst
# Tell Git not to attempt any end-of-line conversions upon checkin or checkout
* -text
*.pdf filter=lfs diff=lfs merge=lfs -text
www/assets/fonts/*.woff filter=lfs diff=lfs merge=lfs -text
www/assets/fonts/*.woff2 filter=lfs diff=lfs merge=lfs -text
www/assets/fonts/*.ttf filter=lfs diff=lfs merge=lfs -text
